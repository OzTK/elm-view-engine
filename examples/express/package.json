{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "npm-watch build:views",
    "build:views": "elm-view-engine"
  },
  "watch": {
    "build:views": {
      "patterns": [
        "views"
      ],
      "extensions": "elm",
      "quiet": false
    }
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "elm": "^0.19.0",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "npm-watch": "^0.7.0",
    "serve-favicon": "~2.4.2"
  }
}
